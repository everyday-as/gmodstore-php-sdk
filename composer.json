{
    "name": "everyday/gmodstore-sdk",
    "type": "library",
    "description": "Welcome to the GmodStore API! You can use our API to access GmodStore API endpoints, which can be used interact with GmodStore programmatically.",
    "keywords": [
        "gmodstore",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/everyday-as/gmodstore-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Everyday AS and contributors",
            "email": "system@everyday.no",
            "homepage": "https://everyday.no"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Everyday\\GmodStore\\Sdk\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Everyday\\GmodStore\\Sdk\\" : "test/" }
    }
}